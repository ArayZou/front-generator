{
  "name": "front-generator",
  "version": "1.0.0",
  "description": "best FE generator in my mind with webpack,less,babel",
  "main": "webpack.config.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js",
    "build": "rm -rf dist && webpack -p --progress --colors",
    "hotbuild": "rm -rf dist && webpack -p --progress --colors --watch",
    "dist": "rm -rf dist && export NODE_ENV=prod&&webpack -p --progress --colors",
    "dist-windows": "rm -rf dist && SET NODE_ENV=prod&&webpack -p --progress --colors",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArayZou/front-generator.git"
  },
  "keywords": [
    "webpack",
    "less",
    "babel"
  ],
  "author": "arayzou",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ArayZou/front-generator/issues"
  },
  "homepage": "https://github.com/ArayZou/front-generator#readme",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "glob": "^7.0.5",
    "happypack": "^2.2.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "raw-loader": "^0.5.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "jquery": "^3.1.0"
  }
}
